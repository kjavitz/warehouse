<?xml version="1.0"?>
<config>
    <modules>
        <ITwebexperts_PPRWarehouse>
            <version>0.1.0</version>

        </ITwebexperts_PPRWarehouse>
    </modules>
    <global>
        <models>
            <pprwarehouse>
                <class>ITwebexperts_PPRWarehouse_Model</class>
                <resourceModel>pprwarehouse_resource</resourceModel>
            </pprwarehouse>
            <pprwarehouse_resource>
                <class>ITwebexperts_PPRWarehouse_Model_Resource</class>
            </pprwarehouse_resource>
            <payperrentals>
                <rewrite>
                    <product_backend_serialnumbers>ITwebexperts_PPRWarehouse_Model_Payperrentals_Product_Backend_Serialnumbers</product_backend_serialnumbers>
                    <observer>ITwebexperts_PPRWarehouse_Model_Payperrentals_Observer</observer>
                </rewrite>
            </payperrentals>
            <cataloginventory>
                <rewrite>
                    <stock>ITwebexperts_PPRWarehouse_Model_CatalogInventory_Stock</stock>
                    <stock_item>ITwebexperts_PPRWarehouse_Model_CatalogInventory_Stock_Item</stock_item>
                    <observer>ITwebexperts_PPRWarehouse_Model_CatalogInventory_Observer</observer>
                </rewrite>
            </cataloginventory>
            <sales>
                <rewrite>
                    <quote_item>ITwebexperts_PPRWarehouse_Model_Sales_Quote_Item</quote_item>
                    <!--<quote>ITwebexperts_PPRWarehouse_Model_Sales_Quote</quote>-->
                </rewrite>
            </sales>
            <cataloginventory_mysql4>
                <rewrite>
                    <stock_item>ITwebexperts_PPRWarehouse_Model_Mysql4_Cataloginventory_Stock_Item</stock_item>
                    <!--<stock_status>Innoexts_Warehouse_Model_Mysql4_Cataloginventory_Stock_Status</stock_status>-->
                </rewrite>
            </cataloginventory_mysql4>
           <!-- <cataloginventory_resource>
                <rewrite>
                    <stock_item>Mage_CatalogInventory_Model_Resource_Stock_Item</stock_item>
                </rewrite>
            </cataloginventory_resource>
            loadByProductId we could do this and have a default magento model to be loaded.
            Only problem is how to get the needed start_end_dates
            -->
        </models>
        <blocks>
            <pprwarehouse>
                <class>ITwebexperts_PPRWarehouse_Block</class>
            </pprwarehouse>
            <warehouse>
                <rewrite>
                    <adminhtml_catalog_product_edit_tab_inventory_renderer>ITwebexperts_PPRWarehouse_Block_Warehouse_Adminhtml_Catalog_Product_Edit_Tab_Inventory_Renderer</adminhtml_catalog_product_edit_tab_inventory_renderer>
                    <adminhtml_catalog_product_edit_tab_inventory>ITwebexperts_PPRWarehouse_Block_Warehouse_Adminhtml_Catalog_Product_Edit_Tab_Inventory</adminhtml_catalog_product_edit_tab_inventory>
                </rewrite>
            </warehouse>
            <payperrentals>
                <rewrite>
                    <adminhtml_catalog_product_edit_tab_payperrentals_serialnumbers>ITwebexperts_PPRWarehouse_Block_Payperrentals_Adminhtml_Catalog_Product_Edit_Tab_Payperrentals_Serialnumbers</adminhtml_catalog_product_edit_tab_payperrentals_serialnumbers>
                    <adminhtml_html_sendreturngridproduct>ITwebexperts_PPRWarehouse_Block_Payperrentals_Adminhtml_Html_Sendreturngridproduct</adminhtml_html_sendreturngridproduct>
                    <adminhtml_html_sendreturngridcustomer>ITwebexperts_PPRWarehouse_Block_Payperrentals_Adminhtml_Html_Sendreturngridcustomer</adminhtml_html_sendreturngridcustomer>
                    <adminhtml_quantityreport>ITwebexperts_PPRWarehouse_Block_Payperrentals_Adminhtml_Quantityreport</adminhtml_quantityreport>
                </rewrite>
            </payperrentals>
        </blocks>
        <helpers>
            <pprwarehouse>
                <class>ITwebexperts_PPRWarehouse_Helper</class>
            </pprwarehouse>
            <payperrentals>
                <rewrite>
                    <data>ITwebexperts_PPRWarehouse_Helper_Payperrentals_Data</data>
                    <inventory>ITwebexperts_PPRWarehouse_Helper_Payperrentals_Inventory</inventory>
                </rewrite>
            </payperrentals>
        </helpers>
        <resources>
            <pprwarehouse_setup>
                <setup>
                    <module>ITwebexperts_PPRWarehouse</module>
                    <class>Mage_Catalog_Model_Resource_Setup</class>
                </setup>
            </pprwarehouse_setup>
        </resources>
        <events>
            <sales_order_save_before>
                <observers>
                    <pprwarehouse>
                        <type>singleton</type>
                        <class>pprwarehouse/observer</class>
                        <method>salesOrderSaveBefore</method>
                    </pprwarehouse>
                </observers>
            </sales_order_save_before>
            <after_getquantity>
                <observers>
                    <pprwarehouse_getquantity>
                        <type>singleton</type>
                        <class>pprwarehouse/observer</class>
                        <method>getMaxQuantity</method>
                    </pprwarehouse_getquantity>
                </observers>
            </after_getquantity>
            <ppr_set_stock>
                <observers>
                    <pprwarehouse_pprsetstock>
                        <type>singleton</type>
                        <class>pprwarehouse/observer</class>
                        <method>setStock</method>
                    </pprwarehouse_pprsetstock>
                </observers>
            </ppr_set_stock>
            <ppr_before_filter_order>
                <observers>
                    <pprwarehouse_pprfilter>
                        <type>singleton</type>
                        <class>pprwarehouse/observer</class>
                        <method>beforeFilterOrder</method>
                    </pprwarehouse_pprfilter>
                </observers>
            </ppr_before_filter_order>
            <calendar_newelements>
                <observers>
                    <itwebexperts_events1>
                        <class>pprwarehouse/observer</class>
                        <method>calendarNewElements</method>
                    </itwebexperts_events1>
                </observers>
            </calendar_newelements>
            <options_gridnames>
                <observers>
                    <pprwarehouse_8>
                        <class>pprwarehouse/observer</class>
                        <method>optionsGridNames</method>
                    </pprwarehouse_8>
                </observers>
            </options_gridnames>
            <options_grid>
                <observers>
                    <pprwarehouse_9>
                        <class>pprwarehouse/observer</class>
                        <method>optionsGrid</method>
                    </pprwarehouse_9>
                </observers>
            </options_grid>
            <options_gridcols>
                <observers>
                    <pprwarehouse_10>
                        <class>pprwarehouse/observer</class>
                        <method>optionsGridCols</method>
                    </pprwarehouse_10>
                </observers>
            </options_gridcols>
            <options_gridbuttons>
                <observers>
                    <pprwarehouse_11>
                        <class>pprwarehouse/observer</class>
                        <method>optionsGridButtons</method>
                    </pprwarehouse_11>
                </observers>
            </options_gridbuttons>
        </events>
    </global>
    <frontend>
        <routers>
            <payperrentals_front>
                <args>
                    <modules>
                        <itwebexperts_pprwarehouse before="ITwebexperts_Payperrentals">ITwebexperts_PPRWarehouse</itwebexperts_pprwarehouse>
                    </modules>
                </args>
            </payperrentals_front>
        </routers>
        <layout>
            <updates>
                <itwebexperts_pprwarehouse>
                    <file>pprwarehouse.xml</file>
                </itwebexperts_pprwarehouse>
            </updates>
        </layout>
    </frontend>
    <adminhtml>
        <layout>
            <updates>
                <itwebexperts_pprwarehouse>
                    <file>pprwarehouse.xml</file>
                </itwebexperts_pprwarehouse>
            </updates>
        </layout>
    </adminhtml>
    <admin>
        <routers>
            <payperrentals>
                <args>
                    <modules>
                        <itwebexperts_pprwarehouse before="ITwebexperts_Payperrentals">ITwebexperts_PPRWarehouse</itwebexperts_pprwarehouse>
                    </modules>
                </args>
            </payperrentals>
            <adminhtml>
                <args>
                    <modules>
                        <excellence_salesgrid before="Mage_Adminhtml">ITwebexperts_PPRWarehouse_Adminhtml</excellence_salesgrid>
                    </modules>
                </args>
            </adminhtml>
        </routers>
    </admin>
</config>
